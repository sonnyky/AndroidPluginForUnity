        android:label="@string/app_name"
        android:supportsRtl="true">

        <service android:name="tinker.unityplugin.SpeechRecognizerPlugin">
            <intent-filter>
                <action android:name="tinker.unityplugin.SpeechRecognizerPlugin.intent.RECEIVE"></action>
            </intent-filter>

        </service>

    </application>



</manifest>
